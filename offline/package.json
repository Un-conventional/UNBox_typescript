{
  "name": "UltraBox Offline",
  "version": "2.2.4",
  "description": "An offline version of UltraBox",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . --platform=darwin,linux,win32 --arch=ia32,x64 --asar --overwrite",
    "package-host": "electron-packager . --asar --overwrite"
  },
  "icon": "path.join(__dirname, 'icon.ico')",
  "author": "Main",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.2.6"
  },
  "dependencies": {
    "electron-packager": "^17.1.2"
  }
}
